algoritmo "dia 25/04"

var
qtdeminima,qtdemaxima,estoquemedio:real

inicio
// Seção de Comandos 

escreval("digite o valor da qtde minima:")
leia(qtdeminima)
escreval("digite o qtde da maxima:")
leia(qtdemaxima)
//processamento
estoquemedio<-(qtdeminima+qtdemaxima)/2
//dados de saída
escreval("o estoque médio é: ",estoquemedio)




fimalgoritmo
