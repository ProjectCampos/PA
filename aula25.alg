algoritmo "dia 25/04"

var
valor:real
qtde:inteiro
vt:real

inicio
// Seção de Comandos 

escreval("digite o valor da peça:")
leia(valor)
escreval("digite o quantidade da peça:")
leia(qtde)
//processamento
vt<-qtde*valor
//dados de saída
escreval("o valor total é: ",vt)




fimalgoritmo
